{
    "name": "AdBlockerExtension",
    "description": "Blocks ads",
    "version": "1.0",
    "manifest_version": 3,
    
    "host_permissions": [
        "*://*/*"
    ],

    "permissions": [
        "declarativeNetRequest",
        "declarativeNetRequestFeedback"
    ],

    // "background": {
    //     "service_worker": "background.js"
    // },

    "action": {
        "default_icon": "images/eagles-icon.png",
        "default_popup": "background.js"
    }
}


